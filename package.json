{
  "name": "mix-acuity",
  "productName": "MIX Acuity",
  "version": "1.0.7",
  "author": "Jonathan Brown <jbrown@mix-blockchain.org>",
  "description": "Reference application for MIX Blockchain.",
  "license": "Apache-2.0",
  "main": "./dist/electron/main.js",
  "scripts": {
    "dev": "electron-webpack dev",
    "compile": "electron-webpack",
    "dist": "yarn compile && electron-builder",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null",
    "install": "node --experimental-modules download.mjs",
    "postinstall": "patch-package && electron-builder install-app-deps"
  },
  "build": {
    "asarUnpack": [
      "**/node_modules/sharp/**/*"
    ],
    "mac": {
      "target": [
        "dmg",
        "pkg",
        "zip"
      ]
    },
    "win": {
      "target": [
        "nsis",
        "portable",
        "7z"
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "snap",
        "tar.xz"
      ],
      "category": "Network"
    }
  },
  "dependencies": {
    "@mdi/font": "^2.8.94",
    "@parity/api": "^5.1.5",
    "axios": "^0.18.1",
    "babel-runtime": "^6.26.0",
    "base-58": "^0.0.1",
    "bip32": "^2.0.3",
    "bip39": "^3.0.2",
    "buefy": "^0.8.2",
    "download": "^7.1.0",
    "electron-window-state": "^5.0.3",
    "ethereumjs-tx": "^1.3.7",
    "events": "^3.0.0",
    "google-protobuf": "^3.5.0",
    "iltorb": "^2.4.0",
    "just-throttle": "^1.1.0",
    "keythereum": "^1.0.4",
    "level": "^5.0.1",
    "multihashes": "^0.4.13",
    "qrcode": "^1.2.0",
    "request": "^2.88.0",
    "sharp": "^0.22.1",
    "source-map-support": "^0.5.12",
    "twemoji": "12.0.4",
    "unused-filename": "^2.1.0",
    "vue": "^2.6.10",
    "vue-i18n": "^8.10.0",
    "vue-markdown": "^2.2.4",
    "vue-recaptcha": "^1.2.0",
    "vue-router": "^3.0.1",
    "vue-simple-progress": "^1.1.0",
    "vue-timeago": "^5.0.0",
    "web3": "^2.0.0-alpha.1"
  },
  "devDependencies": {
    "electron": "^6.0.10",
    "electron-builder": "^20.44.4",
    "electron-webpack": "^2.7.2",
    "electron-webpack-ts": "^3.2.0",
    "electron-webpack-vue": "^2.3.0",
    "node-sass": "^4.12.0",
    "patch-package": "^6.1.4",
    "postinstall-postinstall": "^2.0.0",
    "sass-loader": "^7.0.1",
    "typescript": "^3.5.2",
    "webpack": "^4.33.0"
  }
}
